        }
      },
      "id": "cc106fa0-6630-4c84-aea4-a4c7a3c149e9",
      "name": "Search Airtable",
      "type": "n8n-nodes-base.airtable",
      "typeVersion": 2,
      "position": [1000, 600]
    },
    {
      "parameters": {
