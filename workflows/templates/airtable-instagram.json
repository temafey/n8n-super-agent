          "cachedResultName": "Instagram Profiles"
        },
        "filterByFormula": "={{ \"AND(SEARCH('\" + $json.searchQuery.toLowerCase() + \"', LOWER({Username})), {Followers} >= \" + ($json.minFollowers || 0) + \")\" }}",
        "options": {
          "fields": [
            {
              "field": "Username",
              "included": true
            },
            {
              "field": "Followers",
              "included": true
            },
            {
              "field": "Bio",
              "included": true
            },
            {
              "field": "Email",
              "included": true
            },
            {
              "field": "Categories",
              "included": true
            },
            {
              "field": "Engagement Rate",
              "included": true
